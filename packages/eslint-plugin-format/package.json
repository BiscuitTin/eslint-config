{
  "name": "@biscuittin/eslint-plugin-format",
  "type": "module",
  "version": "0.0.1",
  "description": "Format various languages with formatters in ESLint, a fork of Antfu's, with support for configuration files.",
  "author": "Biscuit Tin <opensource@biscuitt.in>",
  "license": "MIT",
  "homepage": "https://github.com/BiscuitTin/eslint-config/tree/main/packages/eslint-plugin-format#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BiscuitTin/eslint-config.git",
    "directory": "packages/eslint-plugin-format"
  },
  "bugs": "https://github.com/BiscuitTin/eslint-config/issues",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "formatters",
    "prettier",
    "dprint"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./define-config-support": "./dist/define-config-support.js",
    "./rule-options": "./dist/rule-options.js",
    "./workers/dprint": "./dist/workers/dprint.js",
    "./workers/prettier": "./dist/workers/prettier.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "publishConfig": {
    "provenance": true
  },
  "scripts": {
    "build": "tsdown"
  },
  "peerDependencies": {
    "@dprint/formatter": "^0.4.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": false
    },
    "prettier": {
      "optional": true
    }
  },
  "dependencies": {
    "@biscuittin/eslint-parser-plain": "workspace:^",
    "eslint-formatting-reporter": "^0.0.0",
    "find-up-simple": "^1.0.1",
    "micro-memoize": "^4.2.0",
    "synckit": "^0.11.11",
    "valibot": "1.1.0"
  },
  "devDependencies": {
    "@dprint/formatter": "^0.4.1",
    "eslint": "^9.37.0",
    "prettier": "^3.6.2"
  }
}
