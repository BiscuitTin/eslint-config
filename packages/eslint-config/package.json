    "@eslint/config-inspector": "^0.6.0",
    "@types/eslint-plugin-react-refresh": "^0.4.0",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@typescript-eslint/parser": "^8.18.1",
    "dprint": "^0.47.6",
    "eslint": "^9.17.0"
