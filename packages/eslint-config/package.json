    "eslint-plugin-react-web-api": "^1.52.5",
    "eslint-plugin-react-x": "^1.52.5",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-unicorn": "^60.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "find-up": "^7.0.0",
    "globals": "^16.3.0",
