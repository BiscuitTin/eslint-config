{
  "name": "@bttin/eslint-config",
  "type": "module",
  "version": "0.1.0",
  "description": "A collection of ESLint configurations for Biscuit Tin projects.",
  "author": "Biscuit Tin <opensource@biscuitt.in>",
  "license": "MIT",
  "repository": {
    "type": "git",
