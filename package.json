{
  "name": "biscuit-tin-eslint-config",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.2.1",
  "author": "Biscuit Tin <opensource@biscuitt.in>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BiscuitTin/eslint-config.git"
  },
  "engines": {
    "node": ">=22.13.1 <23",
    "bun": ">=999.0.0",
    "npm": ">=999.0.0",
    "yarn": ">=999.0.0"
  },
  "scripts": {
    "build": "jiek build --noMin --filter @biscuittin/*",
    "test": "vitest --run --logHeapUsage",
    "coverage": "vitest run --coverage",
    "typegen": "tsx scripts/typegen.ts",
    "lint": "eslint --cache .",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biscuittin/eslint-config": "workspace:^",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@commitlint/types": "^19.5.0",
    "@types/lint-staged": "^13.3.0",
    "@types/node": "^22.13.1",
    "@vitest/coverage-v8": "^3.0.5",
    "dprint": "^0.49.0",
    "eslint": "^9.19.0",
    "eslint-typegen": "^1.0.0",
    "happy-dom": "^17.0.3",
    "husky": "^9.1.7",
    "jiek": "^2.3.3",
    "jiti": "^2.4.2",
    "lint-staged": "^15.4.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "pnpm": {
    "overrides": {
      "deep-equal": "npm:@nolyfill/deep-equal@^1",
      "eslint": "9.19.0",
      "is-core-module": "npm:@nolyfill/is-core-module@^1",
      "is-generator-function": "npm:@nolyfill/is-generator-function@^1",
      "safe-buffer": "npm:@nolyfill/safe-buffer@^1",
      "safer-buffer": "npm:@nolyfill/safer-buffer@^1"
    }
  }
}
