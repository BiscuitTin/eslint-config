{
  "name": "biscuit-tin-eslint-config",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.13.1",
  "author": "Biscuit Tin <opensource@biscuitt.in>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BiscuitTin/eslint-config.git"
  },
  "engines": {
    "node": ">=22.13.1 <23",
    "bun": ">=999.0.0",
    "npm": ">=999.0.0",
    "yarn": ">=999.0.0"
  },
  "scripts": {
    "build": "pnpm --recursive --filter @biscuittin/* run build",
    "test": "vitest --run --logHeapUsage",
    "coverage": "vitest run --coverage",
    "typegen": "tsx scripts/typegen.ts",
    "lint": "eslint --cache .",
    "prepare": "husky"
  },
  "devDependencies": {
    "@arethetypeswrong/core": "^0.18.2",
    "@biscuittin/eslint-config": "workspace:^",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/types": "^19.8.0",
    "@types/lint-staged": "^13.3.0",
    "@types/node": "^22.14.0",
    "@vitest/coverage-v8": "^3.1.1",
    "dprint": "^0.49.1",
    "eslint": "^9.23.0",
    "eslint-typegen": "^2.1.0",
    "happy-dom": "^17.4.4",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^15.5.0",
    "publint": "^0.3.12",
    "tsdown": "^0.13.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.1.1"
  },
  "pnpm": {
    "overrides": {
      "is-core-module": "npm:@nolyfill/is-core-module@^1"
    }
  }
}
